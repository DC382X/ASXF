[wiki=(wiki page)][/wiki] - 
[wikipedia=(wikipedia page)][/wikipedia] - 
[wp=(wikipedia page)][/wp] - 
[google=(term)][/google] - 
[dictonary=(word)][/dictonary] - 
[dic=(word)][/dic] - 
[code][/code] - 
[code=python][/code] - 
[code=JSON][/code] - 
